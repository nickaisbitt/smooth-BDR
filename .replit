modules = ["web", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["javascript_openrouter_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["sqlite"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start Application"

[[workflows.workflow]]
name = "Start Application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node agents/supervisor.js & node server.js & npm run dev"
waitForPort = 5000

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 37543
externalPort = 3002

[[ports]]
localPort = 46159
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["node", "server.js"]
build = ["npm", "run", "build"]
